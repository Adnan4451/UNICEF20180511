@model WMS.Models.Department

@{
    ViewBag.Title = "Department Create";
}
<br />

@using (Html.BeginForm()) 
{   
     <div class="well bs-component">
        <form class="form-horizontal" action="~/Views/Dept/Create" enctype="multipart/form-data" method="post">
             @Html.AntiForgeryToken()
            <fieldset>
                <div>
      <form class ="form-inline" role="form">
          <div class="form-group "> <h3>Add Department</h3> </div>
      </form>  
    </div> 
                <hr />
    <div class="form-horizontal">
        @Html.ValidationSummary(true)

<div class="form-group">
          
            <div class="col-md-10">
                
                
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">Department</div>
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.DeptName,new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.DeptName)
            </div>
        </div>

        

       <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default btn-success" />
                 @Html.ActionLink("Back to List", "Index",null,new { @class = "btn btn-primary " })
            </div>
        </div>
    </div>
                </fieldset>
            </form>
         </div>
}

<script src="@Url.Content("~/Scripts/HR/DivisionCompanyCreate.js")"></script>
<style>
    hr {
        color: #fff;
        background-color: #fff;
        height: 1px;
    }
</style>